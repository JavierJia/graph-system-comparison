set terminal postscript eps size 4.5,3.0  font 'Helvetica,30' linewidth 2
set output "iteration_connector_32.eps"

set key left top
#set title "32-Machine Cluster"
set xlabel "Dataset Size / Aggregated RAM"
set ylabel "Avg. Iteration Time (Secs)"
set xrange [0:0.7]
set yrange [0:900]
set xtics 0,0.1,0.7

plot "iteration_connector_32.txt" u 1:2 w lp pt 1 ps 2 title "M-to-N Hash","iteration_connector_32.txt" u 1:3 w lp pt 4 ps 2 title "M-to-N Hash Merge"
