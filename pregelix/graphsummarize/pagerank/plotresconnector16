set terminal postscript eps size 4.5,3.0  font 'Helvetica,30' linewidth 2
set output "response_connector_16.eps"

set key left top
#set title "32-Machine Cluster"
set xlabel "Dataset Size / Aggregated RAM"
set ylabel "Execution Time (Seconds)"
set xrange [0:0.7]
#set yrange [0:5000]
set xtics 0,0.1,0.7

plot "response_connector_16.txt" u 1:2 w lp pt 1 ps 2 title "Unmerge","response_connector_16.txt" u 1:3 w lp pt 4 ps 2 title "Merge"
