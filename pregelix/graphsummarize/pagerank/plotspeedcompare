set terminal postscript eps size 4.5,3.0  font 'Helvetica,30' linewidth 2
set output "speedupcompare.eps"

#set title "32-Machine Cluster"
set xlabel "Number of Machines"
set ylabel "Relative Avg Iteration Time"
set xrange [8:32]
set yrange [0:1.2]
set xtics 8,8,32

plot "speedupcompare.txt" u 1:2 w lp pt 2 ps 2 title "Pregelix", "speedupcompare.txt" u 1:3 w lp pt 4 ps 2 title "Giraph-mem", "speedupideal" u 1:2 w lp pt 7 ps 2 title "Ideal"
